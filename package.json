{
	"name": "sveltekit-hcaptcha-form",
	"version": "0.0.1",
	"license": "BSD-3-Clause",
	"description": "SvelteKit hCaptcha contact form: see how you can scare bots away from your Svelte site using a privacy focussed captcha service.",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview --port 3000",
		"check": "svelte-check --fail-on-hints",
		"check:watch": "svelte-check --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
		"format": "prettier --write --plugin-search-dir=. .",
		"generate:images": "node ./generate-responsive-image-data.js",
		"generate:manifest": "node ./generate-manifest.js",
		"generate:sitemap": "node ./generate-sitemap.js",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"prepare": "husky install",
		"svelte-check": "svelte-check"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.0.3",
		"@fontsource/lato": "^4.5.8",
		"@fontsource/lora": "^4.5.10",
		"@fontsource/source-sans-pro": "^4.5.10",
		"@iconify/svelte": "^2.2.1",
		"@rodneylab/sveltekit-components": "^0.2.13",
		"@sveltejs/adapter-static": "^1.0.0-next.34",
		"@sveltejs/kit": "^1.0.0-next.357",
		"@types/object-hash": "^2.2.1",
		"commitlint": "^17.0.3",
		"dayjs": "^1.11.3",
		"dotenv": "^16.0.1",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"front-matter": "^4.0.2",
		"husky": "^8.0.1",
		"mdsvex": "^0.10.6",
		"object-hash": "^3.0.0",
		"postcss": "^8.4.14",
		"prettier": "~2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"reading-time": "1.4.0",
		"sass": "^1.53.0",
		"sharp": "~0.30.7",
		"stylelint": "^14.9.1",
		"stylelint-config-recommended-scss": "^7.0.0",
		"stylelint-config-standard": "^26.0.0",
		"stylelint-scss": "^4.2.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"svelte-preprocess-import-assets": "^0.1.6",
		"vanilla-lazyload": "^17.8.2",
		"vite": "^3.0.4",
		"vite-imagetools": "^4.0.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/sveltekit-hcaptcha-form"
	},
	"bugs": {
		"url": "https://github.com/rodneylab/sveltekit-hcaptcha-form/issues"
	},
	"type": "module",
	"dependencies": {
		"postcss-html": "^1.4.1"
	},
	"peerDependencies": {
		"vite": "^3.0.4"
	}
}
